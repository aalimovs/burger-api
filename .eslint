{
  "extends": "airbnb",
  "rules": {
    "indent": [2, "tab", {"SwitchCase": 1}],
    "object-shorthand": 0,
    "max-len": 0,
    "strict": 0,
    "prefer-template": 0,
    "global-require": 0,
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "no-shadow": 0,
    "spaced-comment": 0,
    "no-trailing-spaces": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign
    "radix": 0,
    "no-use-before-define": 0,
    "no-case-declarations": 0,
    "newline-per-chained-call": 0,
    "no-unused-vars": [1, { "vars": "local", "args": "none" }],
    "new-cap": 0
  }
}